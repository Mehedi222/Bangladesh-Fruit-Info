<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fruitinfoapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo84"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo84"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SearchFruitDetail"></activity>
        <activity android:name=".AboutUs" />
        <activity android:name=".Feedback" />
        <activity android:name=".FruitDetails" />
        <activity android:name=".FruitRelatedVideos" />
        <activity android:name=".AllFruits" />
        <activity android:name=".SearchFruits" />
        <activity
            android:name=".Homepage"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>